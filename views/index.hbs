    <main id="hero">
        <img src="img/hero.jpg" alt="burger & knife">
        <section class="hero">
            <div>
                <h1>World's <span>#1</span> Awesome Food.</h1>
                <p>Deliver you the top quality food all over the world.</p>
                <button onclick="window.location.href='#content'">Get Started</button>
            </div>
        </section>
    </main>

    <main id="content">
        <section class="content">
            <div>
                <h2>How it <span>works?</span></h2>
            </div>
            <div class="flex">
                <div class="grids">
                    <img src="img/content-1.jpg" alt="genuine">
                    <h3>We made genuine recipes.</h3>
                    <p>Provide you best and pure natural ingredients food.</p>
                </div>
                <div class="grids">
                    <img src="img/content-2.jpg" alt="service">
                    <h3>Our service is 24/7.</h3>
                    <p>Provide you the best service 24/7 all over the world.</p>
                </div>
                <div class="grids">
                    <img src="img/content-3.jpg" alt="special">
                    <h3>Our special recipe every weekend.</h3>
                    <p>Provide you our very special legendary recipe by every end of the week.</p>
                </div>
                <div class="grids">
                    <img src="img/content-4.jpg" alt="discounts">
                    <h3>Get upto 70% special discounts.</h3>
                    <p>Provide you the best ever discounts by order more food, and get more discounts.</p>
                </div>
            </div>
        </section>
    </main>

    <main id="topmeals">
        <section class="topmeals">
            <div>
                <h2>Top <span>meals</span></h2>
            </div>
            <div class="flex">
                {{#each data}}
                    {{#mealKits}}
                        {{#topMeal}}
                <div class="grids">
                    <img src="img/{{imageUrl}}" alt="meals">
                    <h3>{{title}}</h3>
                    <p>{{includes}}</p>
                    <a href="#">Continue with ${{price}}</a>
                </div>
                        {{/topMeal}}
                    {{/mealKits}}
                {{/each}}
            </div>
        </section>
    </main>


